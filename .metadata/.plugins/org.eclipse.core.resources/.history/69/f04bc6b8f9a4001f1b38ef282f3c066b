package com.Hibernate.ORM.JavaBasedConfiguration;

public class UserModel {

	private int id;
	private String name;

	public UserModel() {
	}

	public UserModel(String name) {
		super();
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
